"UnlitGeneric"
{
   "$basetexture" "atomic/special/special_agility"
   "$vertexcolor" 		1 
   "$vertexalpha" 		1 
   "$nolod" 			1
   "$translucent" 1
   "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "4"
      }
   }
}